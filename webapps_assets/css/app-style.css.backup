body, p, td, input, select, textarea {
                                       font-family: Garamond;
                                       font-size: 12pt;
 }

body { 
       margin: 0px;
       padding: 0px;
 }

#app-outer-mat { 
                 background-color: white;
                 height: 670px;
                 width: 1080px;
                 border-style: solid;
                 border-color: black;
                 border-width: 1px;
 }

#app-body { 
            height: 600px;
            width: 1080px;
 }
#app-body-sideboard { 
                      float: left;
                      height: 600px;
                      width: 216px;
                      background-color: #9944DD;
                      padding-top: 10px;
 }
#app-body-inner-sideboard { 
                            padding-left: 10px;
 }

#app-body-sidebar-link-container { 
                                   text-align: center;
 }

#app-header { 
              background-color: #cccc99;
              height: 60px;
 }

#app-body-content { 
                    float: right;
                    height: 610px;
                    width: 859px;
                    overflow: auto;
 }
#app-body-inner-content { 
                          
                          padding-left: 10px;
 }

#app-header-text { 
                   padding-top: 15px;
                   margin-left: 16px;
                   font-size: 17pt;
                   font-variant: small-caps;
 }

a:link, a.app-body-sidebar-link:link { 
                                       color: black;
 }
a:hover, a.app-body-sidebar-link:hover { 
                                         color: #0000dd;
 }
a:visited, a.app-body-sidebar-link:visited { 
                                             color: black;
 }
a:visited:hover, a.app-body-sidebar-link:visited:hover { 
                                             color: #0000dd;
 }

a:active, a.app-body-sidebar-link:active { 
                                           color: black;
 }

a.app-body-sidebar-link:link, a.app-body-sidebar-link:hover, a.app-body-sidebar-link:visited, a.app-body-sidebar-link:active  { 
                                                                                                                                text-decoration: none;
 }

#app-footer { 
              float: left;
 }
